{
  "name": "collecthor/spss",
  "description": "SPSS is a php-based implementation of IBM SPSS Statistics Standard",
  "keywords": [
    "SPSS",
    "IBM",
    "PSPP",
    "Statistics",
    "DataSet",
    "por",
    "sav",
    "reader",
    "writer",
    "cli",
    "cli-app"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sam Mousa",
      "email": "sam@mousa.nl"
    },
    {
      "name": "Korniienko Vladyslav",
      "email": "vk.tiamo@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "ext-mbstring": "*",
    "ext-bcmath": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">8",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "SPSS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SPSS\\Tests\\": "tests/"
    }
  }
}
